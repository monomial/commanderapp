<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN"
   "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
   <dict>
       <key>Label</key>
       <string>com.monomial.commanderapp</string>

       <key>ProgramArguments</key>
       <array>
           <string>/usr/local/bin/commanderapp</string>
       </array>

       <key>RunAtLoad</key>
       <true/>

       <key>StandardOutPath</key>
       <string>/tmp/commanderapp.log</string>
       <key>StandardErrorPath</key>
       <string>/tmp/commanderapp.err</string>
   </dict>
</plist>
